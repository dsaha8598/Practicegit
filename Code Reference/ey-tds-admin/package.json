{
  "name": "ey-tds",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng build --prod",
    "start": "npm run env -s && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test --code-coverage --sourceMap=true",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate",
    "generatelintreport": "node lint-reporter.js",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@azure/msal-angular": "^0.1.4",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^3.3.1",
    "@ngx-translate/core": "^10.0.1",
    "@types/adal-angular": "^1.0.1",
    "adal-angular": "^1.0.17",
    "angular-lz-string": "^1.0.5",
    "angulartics2": "^6.0.0",
    "blob-util": "^2.0.2",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "chartjs": "^0.3.24",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "crypto-js": "^4.0.0",
    "datatables.net": "^1.10.19",
    "datatables.net-dt": "^1.10.19",
    "file-saver": "^2.0.2",
    "jquery": "^3.5.0",
    "js-string-compression": "^1.0.1",
    "lodash": "^4.17.20",
    "microsoft-adal-angular6": "^1.3.0",
    "msal": "^1.3.2",
    "ng-lz-string": "^1.5.3",
    "ng-multiselect-dropdown": "0.2.4",
    "ng2-pdf-viewer": "5.3.4",
    "ngx-chips": "2.0.2",
    "ngx-cookie-service": "2.4.0",
    "ngx-logger": "4.1.9",
    "ngx-spinner": "^10.0.1",
    "powerbi-client": "^2.12.1",
    "primeicons": "1.0.0",
    "primeng": "^8.0.0",
    "process": "^0.11.10",
    "readline": "^1.3.0",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.13.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-rocket/scripts": "^3.0.4",
    "@types/datatables.net": "^1.10.19",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.0",
    "@types/node": "^8.10.61",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "^5.0.1",
    "hads": "^1.7.3",
    "htmlhint": "^0.10.3",
    "https-proxy-agent": "^2.2.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "ngx-tslint-report": "^1.2.0",
    "node-sass": "^7.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "^5.4.4",
    "puppeteer": "^1.20.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~7.0.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-html-report": "^2.0.4",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3"
  },
  "browser": {
    "crypto": false,
    "stream": false
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
