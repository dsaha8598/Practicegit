<xs:schema xmlns="http://incometaxindiaefiling.gov.in/common"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://incometaxindiaefiling.gov.in/common"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	xml:lang="EN-US">
	<xs:element name="CreationInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SWVersionNo">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="10" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="SWCreatedBy">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="XMLCreatedBy">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="XMLCreationDate" type="xs:date" />
				<xs:element name="IntermediaryCity">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="25" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Form_Details">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FormName">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="10" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Description">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="0" />
							<xs:maxLength value="1000" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AssessmentYear">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="2017"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="SchemaVer">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="Ver1.1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FormVer">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
